{
  "name": "vela-ui",
  "version": "0.1.0",
  "description": "Vela UI",
  "author": "Vela Team",
  "engines": {
    "node": ">=10.15.0"
  },
  "engineStrict": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@types/parcel-env": "0.0.0",
    "autoprefixer": "^9.7.1",
    "elm": "^0.19.1-3",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.0",
    "sass": "^1.23.3",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "cypress": "^3.6.0",
    "elm-hot": "^1.1.3",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.6",
    "stylelint": "^12.0.0",
    "stylelint-color-format": "^0.2.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.1",
    "stylelint-value-no-exposed-custom-properties": "^2.2.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0"
  },
  "scripts": {
    "predev": "npm run clean",
    "dev": "npm run start",
    "prestart": "npm run clean",
    "start": "parcel src/static/index.html --port 8888",
    "build-img": "parcel build src/static/img/*",
    "prebuild": "npm run clean",
    "build": "parcel build src/static/index.html --no-minify",
    "prebuild-prod": "npm run clean",
    "build-prod": "parcel build src/static/index.html --no-cache --detailed-report --experimental-scope-hoisting",
    "test": "elm-test --compiler ./node_modules/.bin/elm",
    "test:watch": "elm-test --compiler ./node_modules/.bin/elm --watch",
    "test:cy": "start-test 8888 cypress",
    "test:cy-open": "start-test 8888 cypress:open",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "css:lint": "stylelint 'src/scss/**/*.scss' '!src/scss/_reset.scss' --syntax scss",
    "css:lint-fix": "npm run css:lint -- --fix",
    "clean": "rimraf ./dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/go-vela/ui.git"
  },
  "keywords": [
    "elm",
    "parcel",
    "purgecss",
    "cypress"
  ],
  "license": "Apache-2.0"
}
